[push]
	default = simple
[alias]
	s = status
	a = add
	pl = pull --rebase
	ps = push
	o = "!f() { if [ $# -eq 0 ]; then git br; else git checkout $@; fi }; f"
	br = "!f() { if hash branches 2>/dev/null; then branches; else git br; fi }; f"
	b = branch -vv
	l = log
	c = commit
	f = fetch
	r = rebase
	d = diff
	save = !git add --all . && git commit -m "Save" --no-verify
	undo = "!f() { git reset HEAD~$1; }; f"

